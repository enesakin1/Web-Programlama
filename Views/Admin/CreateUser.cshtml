@model myiotprojects.Models.Admin.UserModel
<div class="container bg-white">
    <form asp-controller="Admin" asp-action="CreateUser" method="post">
        <div class="card-header">
            <h4 class="modal-title">@ViewData["Create"] @ViewData["User"]</h4>
        </div>
        <div class="card-body">
            <div class="form-group">
                <label>@ViewData["Nickname"]</label>
                <input asp-for="@Model.Nickname" class="form-control">
            </div>
            <div class="form-group">
                <label>Email</label>
                <input asp-for="@Model.Email" class="form-control">
            </div>
            <div class="form-group">
                <label>@ViewData["Password"]</label>
                <input asp-for="@Model.Password" class="form-control">
            </div>
        </div>
        <div class="card-footer">
            <input type="submit" class="btn btn-info" value="@ViewData["Save"]">
        </div>
    </form>
</div>